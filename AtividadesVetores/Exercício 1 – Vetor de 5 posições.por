programa
{
    funcao inicio()
    {
        inteiro vetor[5]

        vetor[0] = 8
        vetor[1] = 5
        vetor[2] = 10
        vetor[3] = 1
        vetor[4] = -25

        inteiro i
        para (i = 0; i < 5; i++)
        {
            escreva("Posição ", i, " = ", vetor[i], "\n")
        }
    }
}